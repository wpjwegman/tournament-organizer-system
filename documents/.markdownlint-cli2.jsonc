{
  "outputFormatters": [
    [
      "markdownlint-cli2-formatter-default"
    ]
  ],
  "globs": [
    "**/*.md"
  ],
  "ignores": [
    ".venv/**",
    "node_modules/**",
    "**/node_modules/**",
    "site/**",
    "__pycache__/**",
    "**/__pycache__/**",
    ".git/**",
    "markdown_fixes_backup/**",
    "**/licenses/**",
    "**/*.dist-info/**"
  ]
}
